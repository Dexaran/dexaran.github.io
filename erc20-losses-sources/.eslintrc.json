{
  "extends": ["next/core-web-vitals"],
  "overrides": [
    {
      "files": "**/*.+(ts|tsx)",
      "plugins": [
        "prettier",
        "simple-import-sort"
      ],
      "rules": {
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error",
        "prettier/prettier": "error"
      }
    }
  ]
}
